<h1>Create Your Course</h1>

<form action='/createcourse' method ='POST'>
<br>
	<label for='course'>Course Name</label>
	<input type='text' name='coursename'>
<br>
	<label for='course'>Address</label>
	<input type='text' name='address'>
<br>
	Public/Private<select name='public_course'>
		<option value='true'>Public</option>
		<option value='false'>Private</option>
	</select>
<br>
	Number of Holes<select name='numberOfholes'>
			<option value='9'>9</option>
			<option value='18'>18</option>
			<option value='36'>36</option>
	</select>
<br>
	<label for='review'>Course Handicap</label>
	<input type='text' name='coursehandicap'>
<br>
	<label for='review'>Description</label>
	<input type='text' name='description'>	

<br>
	<input type="submit" value="Submit">	

</form>